{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "checkJs": true,
      "allowJs": true,
      "baseUrl": "./static/js",
      "paths": {
        "*": ["*", "engines/*", "core/*", "performance/*"]
      }
    },
    "include": ["static/js/**/*.js", "templates/**/*.html"],
    "exclude": [
      "node_modules",
      ".venv",
      "static/js/tailwindcss.js"
    ]
  }
  